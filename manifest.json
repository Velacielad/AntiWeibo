{
    "manifest_version": 2,

    "name": "AntiWeibo",
    "description": "Anti Weibo, block it when you spend too much time on it.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "storage",
        "*://*.weibo.com/",
        "*://weibo.com/"
    ]
}
